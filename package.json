    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "^28.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.31",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^1.2.0",
    "jest": "^28.1.1",
    "supertest": "^6.3.1",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
